# Check processes
 htop
 ps -ux
 /proc/
 getrusage
# Check memory
 free -h
# Check partitions id
 blkid
# Check partitions
 lsblk
# Check partitions
 sudo fdisk -l
# Disk usage
 du -amd1
# See hardware
 inxi -Fx
# hardware and drivers
 dmesg |grep -i
# hardware and drivers
 inxi -Fx
# current kernel
 uname -r


